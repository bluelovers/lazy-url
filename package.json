{
  "name": "lazy-url",
  "version": "1.0.9",
  "description": "a more easy use URL",
  "keywords": [
    "URL",
    "WHATWG",
    "address",
    "canonical",
    "normalisation",
    "normalization",
    "normalize",
    "parse",
    "parser",
    "query",
    "querystring",
    "simplify",
    "string",
    "stringify",
    "strip",
    "trim",
    "uri",
    "url"
  ],
  "homepage": "https://github.com/bluelovers/lazy-url#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/lazy-url/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/lazy-url.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc npm run test",
    "lint": "npx eslint **/*.ts",
    "test": "jest --coverage",
    "build": "yarn run build:before & yarn run build:tsc & yarn run build:after",
    "build:after": "git add ./esm & git add ./lib & echo build:after",
    "build:before": "git rm ./esm -r -f & echo build:before",
    "build:tsc": "tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run ncu && yarn run sort-package-json && yarn run build",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort"
  },
  "dependencies": {
    "symbol.inspect": "^1.0.1",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/url-parse": "^1.4.3",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0"
  }
}
